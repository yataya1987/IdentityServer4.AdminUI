{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [
        "node"
    ],
    "paths": {
        "@shared/*": [
            "app/shared/*"
        ],
        "@app/*": [
            "app/panel/*"
        ],
        "@core/*": [
            "app/core/*"
        ],
        "@env/*": [
            "environments/*"
        ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
